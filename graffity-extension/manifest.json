{
    "manifest_version": 2,

    "name": "Graffity",
    "description": "This extension lets you draw on webpages and see other people's drawings",
    "version": "1.0",

    "browser_action": {
        "default_icon": "graffity_icon19.png",
        "default_popup": "graffity_popup.html"
    },

    "icons": {
        "19": "graffity_icon19.png",
        "48": "graffity_icon48.png"
    },

    "permissions": [
        "activeTab",
        "http://127.0.0.1/"
    ]
}
